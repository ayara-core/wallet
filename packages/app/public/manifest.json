{
	"name": "Ayara Wallet",
	"description": "The first universal-gas wallet",
	"version": "0.1.0",
	"manifest_version": 3,
	"content_security_policy": {
		"extension_pages": "script-src 'wasm-unsafe-eval' 'self'; object-src 'self' "
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "/icon-ayara.png",
			"48": "/icon-ayara.png",
			"128": "/icon-ayara.png"
		}
	},
	"icons": {
		"16": "/icon-ayara.png",
		"48": "/icon-ayara.png",
		"128": "/icon-ayara.png"
	}
}